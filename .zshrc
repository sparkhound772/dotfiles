
#################################################
# Custom stuff. Append the to the native .zshrc #
#################################################
bindkey '^[[3;5~' forward-kill-word	# Delete word to the right with Ctrl + K
alias py='python3'
alias gl='git --no-pager log --oneline'
alias gb='git --no-pager branch -vv'
alias gs='git status'
alias x='xclip -selection clipboard -o'
alias v='nvim'
alias nv='nvim'
alias vim='nvim'
